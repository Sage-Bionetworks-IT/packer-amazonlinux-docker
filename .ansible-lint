exclude_paths:
  - .
  - temp
  - .github
  - .pre-commit-config.yaml
parseable: true
quiet: true
skip_list:
  - "301"
  - "305"
  - "306"
  - "name[play]"
  - "name[casing]"
  - "fqcn-builtins"
  - "fqcn[action]"
  - "fqcn[action-core]"
  - "syntax-check[unknown-module]"
use_default_rules: true
verbosity: 3
strict: false
